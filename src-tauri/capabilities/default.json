{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for all windows",
  "windows": ["*"],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "core:window:allow-is-focused",
    "core:window:allow-close",
    "core:window:allow-set-size",
    "core:event:default",
    "shell:allow-open"
  ]
}
